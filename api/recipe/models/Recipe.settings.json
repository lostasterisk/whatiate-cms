{
  "connection": "default",
  "collectionName": "recipes",
  "info": {
    "name": "recipe",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "published": {
      "type": "date"
    },
