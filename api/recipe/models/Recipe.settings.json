    },
    "macros": {
      "type": "json"
    },
    "micros": {
      "type": "json"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "video": {
      "type": "string"
    },
    "ingredients": {
      "type": "json"
    }
  }
}